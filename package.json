{
  "name": "@emartech/angular-suite-flipper",
  "version": "1.0.0",
  "description": "Angular module for Suite FLipper",
  "keywords": [
    "angular",
    "angular2",
    "suite",
    "flipper"
  ],
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "test": "mocha --require core-js/shim --require reflect-metadata --require ts-node/register lib/**/*.spec.ts",
    "prepublish": "typings install && rimraf dist && tsc",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "verifyConditions": {
      "path": "./node_modules/semantic-release/dist/lib/plugin-noop.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emartech/angular2-suite-flipper.git"
  },
  "author": "Viktor Somodi <viktor.somodi@emarsys.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emartech/angular2-suite-flipper/issues"
  },
  "homepage": "https://github.com/emartech/angular2-suite-flipper#readme",
  "devDependencies": {
    "@angular/core": "^2.0.0",
    "chai": "3.5.0",
    "core-js": "2.4.1",
    "mocha": "2.4.5",
    "reflect-metadata": "0.1.8",
    "rimraf": "2.5.2",
    "rxjs": "5.0.0-rc.3",
    "semantic-release": "6.2.1",
    "ts-node": "0.7.0",
    "typescript": "2.0.10",
    "typings": "2.0.0",
    "zone.js": "0.6.26"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0"
  },
  "dependencies": {
    "suite-flipper-js": "^2.0.1"
  }
}
